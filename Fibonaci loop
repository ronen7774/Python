#initial start
fin_number = -1
previous_number = []

#choose the range, the second argument is not included
for n in range (1,11):
  if n < 3:
    fin_number = 1
    previous_number.append(fin_number)
  else:
    fin_number = (previous_number[n-2]+previous_number[n-3])
    previous_number.append(fin_number)
#print(previous_number)



  print("n: ",n,"Fibonaci: ",fin_number)
